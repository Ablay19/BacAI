# Frontend Environment Variables
# Copy this file to .env.local and fill in the values

# API Configuration
VITE_API_URL=https://bacai-api-development.abdoullahelvogani.workers.dev

# Optional: Analytics (if using)
# VITE_GA_ID=G-XXXXXXXXXX
# VITE_ANALYTICS_URL=https://analytics.example.com

# Optional: Feature Flags
# VITE_ENABLE_DARK_MODE=true
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_CHAT=true

# Optional: AI Model Configuration
# VITE_MODEL_NAME=qwen-8b
# VITE_MAX_TOKENS=1000
# VITE_TEMPERATURE=0.7

# Optional: App Configuration
# VITE_APP_NAME=BACAI
# VITE_APP_VERSION=1.0.0
# VITE_SUPPORTED_LANGUAGES=ar,fr,en

# Optional: Feature Toggles
VITE_SHOW_PROGRESS=true
VITE_SHOW_ANALYTICS=false
VITE_ENABLE_RTL=true