{
  "version": 2,
  "build": {
    "src": "frontend/package.json",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "dist"
    }
  },
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://bacai-api-development.abdoullahelvogani.workers.dev/api/$1"
    }
  ]
}